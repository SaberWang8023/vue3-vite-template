{
  "name": "vue3-vite-template",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --open",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint src --quiet --cache --ext .tsx,.ts,.js,.jsx,.vue",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "format-prettier": "prettier --write '**/*.{css,gql,graphql,html,js,jsx,json,less,md,mdx,scss,ts,tsx,vue}'",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design-vue/pro-layout": "^3.2.3",
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.12",
    "dayjs": "^1.11.5",
    "lodash.curry": "^4.1.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.22",
    "vite-plugin-pwa": "^0.13.1",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-angular": "^17.1.0",
    "@types/lodash.curry": "^4.1.7",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "eslint": "^8.24.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "typescript": "^4.8.3",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.3",
    "vite-plugin-inspect": "^0.7.4",
    "vue-tsc": "^0.40.13"
  },
  "lint-staged": {
    "src/**/*.{vue,js,jsx,ts,tsx}": [
      "eslint src --quiet --cache --ext .vue,.js,.jsx,.ts,.tsx --fix",
      "prettier --write"
    ]
  }
}
