{
  "name": "vue3-vite-template",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite . open .",
    "dev_qiankun": "cross-env ATTACH_QIANKUN=true vite . --open .",
    "build_proj": "vue-tsc --noEmit && vite build . -m=project",
    "build_test": "vue-tsc --noEmit && vite build . -m=test",
    "build_sml": "vue-tsc --noEmit && vite build . -m=sml",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint src --quiet --cache --ext .tsx,.ts,.js,.jsx,.vue",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "format-prettier": "prettier --write '**/*.{css,gql,graphql,html,js,jsx,json,less,md,mdx,scss,ts,tsx,vue}'",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "codegen": "pont start",
    "plop": "plop --plopfile './scripts/plop/plopfile.mjs' --dest './'"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.0",
    "@vueuse/core": "^10.4.1",
    "ant-design-vue": "^4.0.3",
    "dayjs": "^1.11.9",
    "lodash-es": "^4.17.21",
    "lodash.curry": "^4.1.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-request": "^2.0.3",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-angular": "^17.7.0",
    "@types/lodash-es": "^4.17.9",
    "@types/lodash.curry": "^4.1.7",
    "@types/node": "^18.15.13",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^13.2.3",
    "plop": "^3.1.2",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vite-plugin-qiankun": "^1.0.15",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "1.0.0-rc.4",
    "vue-tsc": "^1.8.11"
  }
}
